generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

model user {
    id          Int       @id @unique @default(autoincrement())
    username    String    @unique
    s_key       String     
    email       String    @unique
    password    String    

}
